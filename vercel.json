{
  "version": 2,

  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "eventMan.settings",
    "PYTHONPATH": "$PYTHONPATH:."
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30,
      "config": {
        "python": "3.11.x"
      }
    }
  }
}
