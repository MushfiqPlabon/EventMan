{
  "version": 2,

  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "eventMan/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "eventMan.settings",
    "PYTHONPATH": "$PYTHONPATH:."
  },
  "functions": {
    "eventMan/wsgi.py": {
      "maxDuration": 30
    }
  }
}
