{
  "version": 2,

  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "eventMan/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "eventMan.settings",
    "PYTHONPATH": "$PYTHONPATH:."
  },
  "functions": {
    "api/eventMan/wsgi.py": {
      "src": "eventMan/wsgi.py",
      "runtime": "python3.12",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/eventMan/wsgi.py"
    }
  ]
}
