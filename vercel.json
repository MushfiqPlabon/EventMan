{
  "version": 2,
  "outputDirectory": "staticfiles",

  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "eventMan.settings",
    "PYTHONPATH": "$PYTHONPATH:."
  },
  "functions": {
    "api/index.py": {
      "runtime": "python@3.12",
      "handler": "api.index.application",
      "maxDuration": 30
    }
  }
}
